@article{barlevy_credit_2003,
	title = {Credit market frictions and the allocation of resources over the business cycle},
	volume = {50},
	issn = {0304-3932},
	url = {http://www.sciencedirect.com/science/article/pii/S0304393203001120},
	doi = {10.1016/j.jmoneco.2002.11.001},
	abstract = {In a seminal paper, Davis and Haltiwanger (1990) demonstrated that recessions are associated with increased job reallocation. The conventional view has interpreted this as evidence of “cleansing”: less productive jobs are destroyed in recessions, and resources are reallocated to more productive uses. This paper argues instead that in the presence of credit market frictions, reallocation might go the other way, directing resources from more efficient to less efficient uses. This will occur if more efficient production arrangements are also more vulnerable to credit constraints. I show that this pattern arises endogenously in an equilibrium model, and offer some evidence that firms with higher output per worker tend to borrow more, suggesting they are more vulnerable to credit constrains.},
	number = {8},
	urldate = {2015-05-08},
	journal = {Journal of Monetary Economics},
	author = {Barlevy, Gadi},
	month = nov,
	year = {2003},
	keywords = {Business cycles, Cleansing, Credit constraints, Job reallocation},
	pages = {1795--1818},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\Wenlan\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\itgns77o.default\\zotero\\storage\\RC9AZX4G\\Barlevy - 2003 - Credit market frictions and the allocation of reso.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\Wenlan\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\itgns77o.default\\zotero\\storage\\TG9249HA\\S0304393203001120.html:text/html}
